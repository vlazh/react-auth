{
  "name": "@vzh/react-auth",
  "version": "1.6.0",
  "description": "React auth components",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "VZH",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy:configs": "copyfiles package.json README.md LICENSE .npmignore ./dist/",
    "build": "npm run clean && tsc --project ./tsconfig.json",
    "build-to-tutoron": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/tutoron/node_modules/@vzh/react-auth/' && copyfiles package.json '/home/vladimir/dev/sources/tutoron/node_modules/@vzh/react-auth/'",
    "patch-publish": "npm run build && npm version patch && npm run copy:configs && npm publish ./dist/ && git push --follow-tags",
    "minor-publish": "npm run build && npm version minor && npm run copy:configs && npm publish ./dist/ && git push --follow-tags",
    "major-publish": "npm run build && npm version major && npm run copy:configs && npm publish ./dist/ && git push --follow-tags"
  },
  "devDependencies": {
    "@types/react": "16.9.35",
    "@types/react-router": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "@vlazh/route-utils": "^0.2.0",
    "@vzh/configs": "^3.53.2",
    "copyfiles": "^2.2.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "history": "^4.10.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-router": "^5.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": ">= 16.x"
  }
}
